    "@babel/core": "^7.19.3",
    "@types/uuid": "^9.0.2",
    "aws-amplify": "^5.3.7",
    "aws-sdk": "^2.1430.0",
    "framer-motion": "^10.16.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^13.4.18",
    "next-auth": "^4.23.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-essentials": "^7.2.1",
